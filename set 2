TYPES OF TRIANGLE............
#include<stdio.h>
int main(){
   int side1, side2, side3;
   printf("Enter sides of triangle:");
   scanf("%d%d%d",&side1,&side2,&side3);
   if(side1 == side2 && side2 == side3)
   {
      printf("The Given Triangle is equilateral");
   } 
   else if(side1 == side2 || side2 == side3 || side3 == side1)
   {
      printf("The given Triangle is isosceles");
   } 
   else
   {
      printf("The given Triangle is scalene");
   } 
   return 0;
}

output//
enter the triangle: 3 4 6
The given triangle is scalene 

LEAP YEAR OR NOT...............
#include <stdio.h>
int main() {
   int year;
   printf("Enter a year: ");
   scanf("%d", &year);
   if (year % 400 == 0) {
      printf("%d is a leap year.", year);
   }
   else if (year % 100 == 0) {
      printf("%d is not a leap year.", year);
   }
   else if (year % 4 == 0) {
      printf("%d is a leap year.", year);
   }
   else {
      printf("%d is not a leap year.", year);
   }
   return 0;
} 

output///
2000
it an leap year

DAYS OF THE  WEEK..........
#include <stdio.h>

int main()
{
    int week;
    printf("Enter week number: ");
    scanf("%d", &week);
    if(week == 1)
    {
        printf("Monday");
    }
    else if(week == 2)
    {
        printf("Tuesday");
    }
    else if(week == 3)
    {
        printf("Wednesday");
    }
    else if(week == 4)
    {
        printf("Thursday");
    }
    else if(week == 5)
    {
        printf("Friday");
    }
    else if(week == 6)
    {
        printf("Saturday");
    }
    else if(week == 7)
    {
        printf("Sunday");
    }
    else
    {
        printf("Invalid Input! Please enter week number between 1-7.");
    }

    return 0;
}  

output// 
5 friday.

ENERGY  BILL CALCULATION.....

#include <stdio.h>
int main() {
    double income, tax = 0;
    printf("Enter the income: ");
    scanf("%lf", & income);
    if (income <= 250000) {
        tax = 0;
    } else if (income <= 500000) {
        tax = (income - 250000) * 0.05;
    } else if (income <= 1000000) {
        tax = 12500 + (income - 500000) * 0.2;
    } else {
        tax = 112500 + (income - 1000000) * 0.3;
    }
    printf("Income tax: %0.2lf", tax);
    return 0;
}

output//
income: 250000
income tax : 0.00

ARTHIMETIC OPERATION.............
#include<stdio.h>
int main()
{
  int n,m,t; 
  char c;
  printf("Enter two numbers and operator :\n");
  scanf("%d %d %c", &n, &m, &c);
  switch(c)
  {
     case 1 : printf("Addition is : %d", n+m);
                break;
     case 2 : printf("Substraction is %d", n-m);
                break;
     case 3 : printf("Multiplication is %d", n*m);
                break;
     case 4 : printf("Division is %f", (float)n/m);
                break;
     default  : printf("Not valid");
  }
 
  getch();
  return 0;
}

output//
2 8 /
division is 0.250000


