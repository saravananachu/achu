DIGITAL ROOTS IN NUMBER.......
#include <stdio.h>
 
int main()
{
  int Number, Sum, Reminder;
 
  printf("\n Please Enter any number\n");
  scanf("%d", &Number);
 
  while(Number >= 10)
  {
    for (Sum=0; Number > 0; Number= Number/10)
    {
      Reminder = Number % 10;
      Sum=Sum + Reminder;  
    }
    if(Sum >= 10)
    {
      Number = Sum;
    }
    else
    {
      printf("\n The Generic Root of a Given Number = %d", Sum);
      return 0;
    }
  } 
}

output//
the number : 56848
the roots are  7.p

FIBANOIC SERIES..............
#include<stdio.h>
void main()
{
  int n,n1=0,n2=1,n3,i;
  printf("enter the number :");
  scanf("%d",&n);
  for(i=0;i<=n;i++)
  { 
    n3=n1+n2;
    printf("%d",n3);
    n1=n2;
    n2=n3;
   }
}

outpit//
50
1
1
2
3

