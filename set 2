ARRAY SORTING................
#include <stdio.h>

void main()
{
    int arr1[100];
    int n, i, j, tmp;
    printf("Input the size of array : ");
    scanf("%d", &n);
       for(i=0;i<n;i++)
            {
	      
	      scanf("%d",&arr1[i]);
	    }

    for(i=0; i<n; i++)
    {
        for(j=i+1; j<n; j++)
        {
            if(arr1[j] <arr1[i])
            {
                tmp = arr1[i];
                arr1[i] = arr1[j];
                arr1[j] = tmp;
            }
        }
    }
    printf("\nElements of array in sorted ascending order:\n");
    for(i=0; i<n; i++)
    {
        printf("%d  ", arr1[i]);
    }
	        printf("\n\n");
}

output\\
5
12
8
6
4
5

4,,5,6,8,12

